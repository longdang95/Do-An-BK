.inventory-info{
  background white;
  border-radius 10px;
  -webkit-box-shadow: 0px 0px 30px -1px rgba(0,0,0,0.11);
  -moz-box-shadow: 0px 0px 30px -1px rgba(0,0,0,0.11);
  box-shadow: 0px 0px 30px -1px rgba(0,0,0,0.11);
  h4{
    margin-block-start: 0 !important
    margin-bottom 0 !important;
    margin 0 !important;
  }
  .inven{
    .inven-right{
      width 100%;
      justify-content space-between
    }
    .inven-left{
      flex 1 ;
    }
    .time {
      flex 1 ;
      text-align right
    }
  }
}