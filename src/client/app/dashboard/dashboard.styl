.dashboard{
  background #fcfcfc;

  padding 20px;
  .sum-header{
    display flex;
    flex-direction row;
    justify-content space-between
  }
  .head-card{
    border-radius 10px;
    padding 10px 0 ;
    background white ;
    width 33%;
    -webkit-box-shadow: 0px 0px 25px -1px rgba(189,189,189,1);
    -moz-box-shadow: 0px 0px 25px -1px rgba(189,189,189,1);
    box-shadow: 0px 0px 25px -1px rgba(189,189,189,1);
    .sum-cover{
      &.sum-content{
        margin 0 10px;
      }
    }
  }

  .payment-infomation{
    padding-top 50px;
    .inventory{
      min-height 300px;
    }
    .payment{
      min-height 300px;
    }
  }
}