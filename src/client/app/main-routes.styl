.main-routes {
  animation faded 1s ease alternate
}

.no-margin {
  margin 0 !important;
}

.no-padding {
  padding 0 !important;
}

.text-wrap {
  overflow-wrap: break-word;
  word-wrap: break-word;
  hyphens: auto;
}

.ct-center {
  width 1200px;
  margin 0 auto;
}
.none-underline{
  &:hover , &:visited ,&:link, &:active {
    text-decoration: none;
  }
}
