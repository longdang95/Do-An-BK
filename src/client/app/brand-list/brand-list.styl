.brand-list{


  .filter-bar{
    padding-top 50px;
    .fil-label, .fil-content{
      border 1px solid #e3e3e3;
    }
    .fil-tab{
      min-width 200px;
      min-height auto;
    }
    .fil-label{
      padding: 5px;
      font-weight: 700;
      background: #f7f7f7;
      border: solid 1px #eee;
    }
    .fil-content{
      padding: 15px;
      border-top: none;
      margin-bottom: 10px;
      background: #fff;
      border-top none !important
      a{
        div{
          padding 2px 0;
        }
      }
    }
  }
}